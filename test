pipeline {
    agent any 
    stages {
        stage name: 'Build', concurrency: 1{
            steps {
                echo 'Build in process' 
            }
        }
        stage name: 'Test', concurrency: 1{
            steps {
                echo 'Testing In process ?' 
            }
       }
       stage('Deploy') {
            steps {
                echo 'Deploying' 
            }
       }
    }
}
