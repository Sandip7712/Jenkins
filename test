pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello world!' 
            }
        }
    }
    stages {
        stage('Stage 2') {
            steps {
                echo 'How Are You ?' 
            }
       }
   }
   stages {
        stage('Stage 3') {
            steps {
                echo 'I am fine' 
            }
       }
   }
 ]
